     type_code : 4
     p_codes :
      -
        code : |1+
         // Version 1
         // Description 
         // Bouton en validation
         
        type : 14
      -
        code : |1-
         
         REQ_A_VOTER.ParamCodeElecteur = nCodeSecret
         REQ_A_VOTER.ParamDateVoter = LIB_Date
         REQ_A_VOTER.ParamHeureVoter = LIB_Heure
         
         // Ajouts de paramètres afin de vérifier si la personne a déjà voté
         REQ_VerifCodeElecteur.ParamCodeElecteur = nCodeSecret
         
         REQ_AjoutVote.ParamCodeCandidat = nNuméroCandidat
         REQ_AjoutVote.ParamCodeElection = 1
         HExécuteRequête(REQ_VerifCodeElecteur)
         // Si le code électeur est retrouvé dans la base, c'est que la personne a déjà voté sinon, le vote est prit en compte
         SI HLitPremier(REQ_VerifCodeElecteur) = Null ALORS
         
         // Ajouts du nombre de vote pour le candidat
         HExécuteRequête(REQ_AjoutVote)
         // Ajouts du fait qu'une personne a voté
         HExécuteRequête(REQ_A_VOTER)
         FEN_Validation_Vote.Ouvre()
         SINON SI HLitPremier(REQ_VerifCodeElecteur) = HTrouve(AVOTER)
         	Ouvre(FEN_Validation_Vote)
         SINON
         	Erreur("Erreur.")
         FIN
